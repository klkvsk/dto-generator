{
    "name": "klkvsk/dto-generator",
    "description": "Generate DTO classes with zero runtime dependencies",
    "type": "library",
    "bin": [
        "bin/dto-gen"
    ],
    "autoload": {
        "psr-4": {
            "Klkvsk\\DtoGenerator\\": "src/"
        },
        "files": [
            "src/Schema/functions.php",
            "src/Schema/Types/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Klkvsk\\DtoGenerator\\Test\\": "tests",
            "Klkvsk\\DtoGenerator\\Example\\": "example/generated"
        }
    },
    "authors": [
        {
            "name": "klkvsk",
            "email": "m@klkvsk.ru"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/php-generator": "^4.0",
        "opis/closure": "^3.6",
        "psr/log": "^3.0",
        "ulrichsg/getopt-php": "^4.0",
        "splitbrain/php-cli": "^1.2",
        "spatie/php-cloneable": "^1.0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.7"
    }
}
